
public class Engine {

	private String input;
	private String CNFform;
	
	public Engine(String theInput){
		input = theInput;
		
		CNFform = convToCNF(theInput);
		
	}
	
	
	
	private String convToCNF(String original){
		int index;
		
		// a<=>b -> (a=>b)&(b=>a) 
		if(original.indexOf("<=>") != -1){
			index = original.indexOf("<=>")
			System.out.println();
		}
			
		
		
		
		
		
		// a => b  -> -a | b
		
		//
	}
	
}
